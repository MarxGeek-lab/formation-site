        testimonial.description;\n        document.getElementById(\n          \"missionText\"\n        ).innerHTML = `\"${testimonial.mission}\"`;\n        document.getElementById(\"avatarImage\").src = testimonial.avatar;\n        document.getElementById(\"clientName\").textContent =\n          testimonial.clientName;\n        document.getElementById(\"resultAmount\").textContent =\n          testimonial.result;\n        document.querySelector(\".period\").textContent = testimonial.period;\n\n        \/\/ Animation d'entr\u00e9e\n        content.classList.remove(\"fade-out\");\n      }, 250);\n    }\n\n    function changeTestimonial(direction) {\n      currentIndex += direction;\n\n      if (currentIndex >= testimonials.length) {\n        currentIndex = 0;\n      } else if (currentIndex < 0) {\n        currentIndex = testimonials.length - 1;\n      }\n\n      updateTestimonial(currentIndex);\n    }\n\n    \/\/ Initialisation\n    updateTestimonial(currentIndex);\n\n    \/\/ Navigation clavier\n    document.addEventListener(\"keydown\", (e) => {\n      if (e.key === \"ArrowLeft\") {\n        changeTestimonial(-1);\n      } else if (e.key === \"ArrowRight\") {\n        changeTestimonial(1);\n      }\n    });\n  <\/script>\n<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-2148323 e-con-full e-flex e-con e-child\" data-id=\"2148323\" data-element_type=\"container\">\n\t\t\t\t<div class=\"elementor-element elementor-element-14dae51 elementor-widget__width-initial g-text-1 elementor-widget elementor-widget-heading bdevs-el heading\" data-id=\"14dae51\" data-element_type=\"widget\" data-settings=\"{&quot;design_style&quot;:&quot;style_1&quot;}\" data-widget_type=\"heading.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\n        \n            <div class=\"section__title-wrapper bdevs-el-content\">\n                <h1 class=\"section__title section__title-4 section__title-4-p-2 section-title section_